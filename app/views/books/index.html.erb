<h1>Livros</h1>


<% @books.each do |book| %>
<table>
    <tr>
       <th>Nome do livro</th>
       <th>Nome do Autor </th>
       <th>Sinopse</th>
       <th>Data de Cadastro</th>
       
    </tr>
    <tr>
        <td><%= book.name %></td>
        <td><%= book.author %></td>
        <td><%= book.synopsis %></td>
        <td><%= book.created_at %></td>
    </tr>
   
</table>
<%end%>